package a3107;
import java.util.ArrayList;
public class CANETAv3 {

	public static void main(String[] args) {
		ArrayList<String> nomes  = new ArrayList<>();
		
		nomes.add("Alice");
		nomes.add("Bob");
		nomes.add("Charlies");
		try {
		System.out.println("Nome: " + nomes.get(3));
		} catch (Exception erro) {
			System.out.println("ERRO");
		}
	}

}
